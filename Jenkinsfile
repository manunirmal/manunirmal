pipeline{
  agent any;
  stages {
    stage('Preparing the environment'){
      steps {
        echo 'preparing environment'
      }
    }
    stage('Code Quality'){
      steps {
        echo 'checking code quality'
      }
    }
    stage('Tests'){
      steps {
        echo 'testing application'
      }
    }
    stage('Build'){
      steps {
        echo 'building application'
      }
    }
    stage('Delivery'){
      steps {
        echo 'delivering the application'
      }
    }
    stage('Deploy'){
      steps {
        echo 'deploying the application'
      }
    }
  }
}
